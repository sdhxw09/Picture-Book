{
    "name": "Picture Book",
    "version": "0.1",
    "description": "Grab linked pictures from a web page",
    "options_page": "options.html",
    "background_page": "background.html",
    "page_action": {
        "default_icon": "icon.png"
    },

    "content_scripts": [
        {
            "matches": ["http://www.engadget.com/*"],
            "js": ["jquery-1.7.1.min.js", "content.js"]
        }
    ],

    "permissions": [
        "tabs"
    ]
}
